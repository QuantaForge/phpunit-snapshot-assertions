{
    "name": "quantaforge/phpunit-snapshot-assertions",
    "description": "Snapshot testing with PHPUnit",
    "keywords": [
        "quantaforge",
        "phpunit-snapshot-assertions",
        "phpunit",
        "snapshot",
        "assert",
        "testing"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Tomáš Selič (Smisch-DEV)",
            "email": "contact@smisch.sk"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "composer-runtime-api": "^2.0",
        "phpunit/phpunit": "^10.0",
        "symfony/property-access": "^5.2|^6.2",
        "symfony/serializer": "^5.2|^6.2",
        "symfony/yaml": "^5.2|^6.2"
    },
    "require-dev": {
        "quantaforge/pixelmatch-php": "dev-main",
        "quantaforge/ray": "^1.37"
    },
    "suggest": {
        "quantaforge/pixelmatch-php": "Required to use the image snapshot assertions"
    },
    "autoload": {
        "psr-4": {
            "QuantaForge\\Snapshots\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QuantaForge\\Snapshots\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-v5": "5.0-dev"
        }
    }

}
